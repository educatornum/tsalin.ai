{
  "name": "tsalin-api",
  "version": "1.0.0",
  "description": "RESTful API built with Express and MongoDB Atlas",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed:industries": "node scripts/seedIndustries.js",
    "seed:positions": "node scripts/seedPositions.js",
    "seed:prolevels": "node scripts/seedProLevels.js",
    "seed:all": "npm run seed:industries && npm run seed:positions && npm run seed:prolevels",
    "import:salary-posts": "node scripts/importSalaryPosts.js",
    "migrate:experience": "node scripts/migrateBackfillExperience.js",
    "generate:new-positions": "node scripts/generateNewPositions.js",
    "seed:majors": "node scripts/seedMajors.js",
    "merge:majors": "node scripts/mergeMajorsWithIndustries.js",
    "seed:majors:with-ids": "node scripts/seedMajorsFromMerged.js",
    "augment:majors": "node scripts/augmentMajors.js",
    "append:missing:majors": "node scripts/appendMissingMajors.js",
    "stats:positions-per-industry": "node scripts/stats.js positions-per-industry",
    "stats:industries-with-zeros": "node scripts/stats.js industries-with-zeros",
    "stats:distinct-salary-positions-per-industry": "node scripts/stats.js distinct-salary-positions-per-industry",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "csv-parse": "^5.5.6",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.8.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}

